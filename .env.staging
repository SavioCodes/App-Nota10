# Staging environment for Nota10
NODE_ENV=production
PORT=3000
APP_LOG_LEVEL=info
SERVER_LOG_LEVEL=info

AUTH_SUPABASE_ENABLED=true
DB_DUAL_WRITE=true
DB_READ_POSTGRES=true
BILLING_MERCADOPAGO_WEB_ENABLED=true
BILLING_NATIVE_IAP_ENABLED=true
BILLING_REVENUECAT_LEGACY_ENABLED=true

DATABASE_URL=postgres://<staging_pool_user>:<password>@<staging_pool_host>:6543/postgres?sslmode=require
DIRECT_DATABASE_URL=postgres://<staging_user>:<password>@<staging_host>:5432/postgres?sslmode=require

SUPABASE_URL=https://<staging-project-ref>.supabase.co
SUPABASE_PUBLISHABLE_KEY=sb_publishable_<staging>
SUPABASE_SERVICE_ROLE_KEY=sb_secret_<staging>
SUPABASE_STORAGE_BUCKET=documents

EXPO_PUBLIC_SUPABASE_URL=https://<staging-project-ref>.supabase.co
EXPO_PUBLIC_SUPABASE_PUBLISHABLE_KEY=sb_publishable_<staging>
EXPO_PUBLIC_AUTH_SUPABASE_ENABLED=true

EXPO_PUBLIC_API_BASE_URL=https://api-staging.nota10.com
CORS_ALLOWED_ORIGINS=https://app-staging.nota10.com

MERCADOPAGO_ACCESS_TOKEN=APP_USR-<staging>
MERCADOPAGO_PUBLIC_KEY=APP_USR-<staging>
MERCADOPAGO_WEBHOOK_SECRET=<staging-secret>
MERCADOPAGO_WEBHOOK_URL=https://api-staging.nota10.com/api/billing/webhook/mercadopago

GEMINI_API_KEY=<staging-gemini-key>
JWT_SECRET=<staging-jwt-secret>
